pragma solidity ^0.8.18;

import {TimelockController} from "@openzeppelin/contracts/governance/TimelockController.sol";

contract TimeLock is TimelockController {
    //minDelay is the time that has to pass before a queued transaction can be executed
    // proposers are the only addresses that can propose and queue transactions
    // executors are the only addresses that can execute queued transactions
    constructor(
        uint256 minDelay,
        address[] memory proposers,
        address[] memory executors
    ) TimelockController(minDelay, proposers, executors, msg.sender) {}
}